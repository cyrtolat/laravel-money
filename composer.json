{
    "name": "cyrtolat/laravel-money",
    "description": "A library of Money and Currency for the Laravel framework",
    "homepage": "https://github.com/cyrtolat/laravel-money",
    "keywords": [
        "laravel",
        "money",
        "currency"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Vladislav Tolkechev",
            "email": "cyrtolat@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "ext-json": "*",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cyrtolat\\Money\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cyrtolat\\Money\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cyrtolat\\Money\\Providers\\MoneyServiceProvider"
            ]
        }
    }
}
